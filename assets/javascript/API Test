//API to retrieve RXUID of a drug
//https://rxnav.nlm.nih.gov/REST/drugs.json?name=effexor

var drug = "effexor";

var queryURL = "https://rxnav.nlm.nih.gov/REST/drugs.json?name=" + drug;

// Performing our AJAX GET request
$.ajax({
    url: queryURL,
    method: "GET"
}).done(function(response) {
        // Storing an array of results in the results variable
        var results = response.data;
        console.log(results);
    };
});











//API for 1 drug
//https://rxnav.nlm.nih.gov/REST/interaction/interaction.json?rxcui=
// 384455&sources=DrugBank



//API for list of drugs
//https://rxnav.nlm.nih.gov/REST/interaction/list.json?rxcuis=207106+152923+656659
// &sources=DrugBank